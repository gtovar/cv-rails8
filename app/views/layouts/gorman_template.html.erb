<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Gilberto Tovar</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" type="image/png" href="<%= cdn_asset('images/favicon.png') %>" />
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Jost:wght@300;400;500;600&display=swap">
    <link rel="stylesheet" href="<%= cdn_asset('css/animate.css') %>">
    <link rel="stylesheet" href="<%= cdn_asset('css/et-lineicons.css') %>">
    <link rel="stylesheet" href="<%= cdn_asset('css/themify-icons.css') %>">
    <link rel="stylesheet" href="<%= cdn_asset('css/bootstrap.css') %>">
    <link rel="stylesheet" href="<%= cdn_asset('css/magnific-popup.css') %>">
    <link rel="stylesheet" href="<%= cdn_asset('css/owl.carousel.min.css') %>">
    <link rel="stylesheet" href="<%= cdn_asset('css/owl.theme.default.min.css') %>">
    <link rel="stylesheet" href="<%= cdn_asset('css/flexslider.css') %>">
    <link rel="stylesheet" href="<%= cdn_asset('css/YouTubePopUp.css') %>">
    <link rel="stylesheet" href="<%= cdn_asset('css/style.css') %>">
    <!-- FOR IE9 below -->
    <!--[if lt IE 9]>
        <script src="js/respond.min.js"></script>
    <![endif]-->

    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <!-- CSS del template -->
    <link rel="stylesheet" href="https://jade-entremet-d1399c.netlify.app/css/style.css">
    <!-- Aquí puedes agregar más CSS si es necesario -->
  </head>

  <body data-spy="scroll" data-target=".navbar" data-offset="50">
    <!-- Preloader -->
    <div class="preloader-bg"></div>
    <div id="preloader">
        <div id="preloader-status">
            <div class="preloader-position loader"> <span></span> </div>
        </div>
    </div>
    <!-- Main -->
    <div class="gorman-page"> <a href="#0" class="js-gorman-nav-toggle gorman-nav-toggle"><i></i></a>
      <!-- Sidebar -->
      <aside class="gorman-aside">
        <%= render 'public_pages/partials/sidebar' %>
      </aside>
      <div class="gorman-main">
        <%= yield %>
        <%= render 'public_pages/partials/footer' %>
      </div>
    </div>
    <!-- Scripts del template -->
    <!-- Agrega aquí más JS si el template los requiere -->
    <!-- Progress-wrapp scroll-button -->
    <div class="progress-wrap">
        <svg class="progress-circle svg-content" width="100%" height="100%" viewBox="-1 -1 102 102">
            <path d="M50,1 a49,49 0 0,1 0,98 a49,49 0 0,1 0,-98" /> 
        </svg>
    </div>
    <!-- jQuery -->
    <script src="<%= cdn_asset('js/jquery.min.js') %>"></script>
    <script src="<%= cdn_asset('js/modernizr-2.6.2.min.js') %>"></script>
    <script src="<%= cdn_asset('js/jquery.easing.1.3.js') %>"></script>
    <script src="<%= cdn_asset('js/bootstrap.min.js') %>"></script>
    <script src="<%= cdn_asset('js/jquery.waypoints.min.js') %>"></script>
    <script src="<%= cdn_asset('js/jquery.flexslider-min.js') %>"></script>
    <script src="<%= cdn_asset('js/sticky-kit.min.js') %>"></script>
    <script src="<%= cdn_asset('js/isotope.js') %>"></script>
    <script src="<%= cdn_asset('js/owl.carousel.min.js') %>"></script>
    <script src="<%= cdn_asset('js/jquery.magnific-popup.min.js') %>"></script>
    <script src="<%= cdn_asset('js/YouTubePopUp.js') %>"></script>
    <script src="<%= cdn_asset('js/custom.js') %>"></script>
    <%= javascript_importmap_tags %>

  </body>
</html>
